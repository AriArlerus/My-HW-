import java.util.Scanner;
public class MultiplicationTable {
    public static void main(String args[]) {
      Scanner input = new Scanner (System.in);
      
      System.out.print("Number : ");
      int n = input.nextInt();
      
      int i = 1;
      
        do {
            System.out.println(n + "*" + i + "=" +n*i);
            i++;
        }
        while ( i <= 12 );

    }
}
