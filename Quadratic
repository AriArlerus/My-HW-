public class Quadratic {
    public static void main(String[] args) {
        
       int a = 1;
       int b = 3;
       int c = -4;
        
        int delta = b * b - 4 * a * c;
        
        double x1 = (-b + Math.sqrt(delta)) / (2 * a);
        double x2 = (-b - Math.sqrt(delta)) / (2 * a);
        
        System.out.println((int)x1);
        System.out.println((int)x2);
           
        }
    }
