import java.util.Scanner;
public class IntDouble {
    public static void main(String[] args) {
        
        Scanner input = new Scanner (System.in);
        
        System.out.print("a = ");
        int a = input.nextInt();
        
        System.out.print("b = ");
        double b = input.nextDouble();
        
        if (a > b) 
            System.out.println(a+ " " + b);
        else
            System.out.println(b+ " " + a);
            
    } 
}
